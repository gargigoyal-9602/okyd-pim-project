{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "compile": "tsc -b --incremental",
    "ios": "npx react-native run-ios",
    "start": "concurrently \"npm run compile -- -w\" \"npx react-native start\"",
    "studio": "studio android",
    "xcode": "open ios/OkydPIM.xcworkspace"
  },
  "dependencies": {
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.2",
    "@react-native-community/google-signin": "^3.0.4",
    "blocks": "0.0.1",
    "components": "0.0.1",
    "react": "16.11.0",
    "react-images-upload": "^1.2.7",
    "react-native": "0.61.3",
    "react-native-fbsdk": "^1.1.2",
    "react-native-modal-activityindicator": "0.0.1",
    "react-native-permissions": "^2.2.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "0.0.5",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^25.1.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.7",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "babel-jest": "25.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.3.4",
    "eslint": "6.6.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "25.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-sonar-reporter": "^2.0.0",
    "react-test-renderer": "16.11.0",
    "ts-jest": "^25.0.0",
    "pixels-catcher": "^0.7.0",
    "react-native-save-view": "^0.2.3"
  },
  "jestSonar": {
    "reportPath": "../../reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/../../node_modules/jest-enzyme/lib/index.js"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "!../../node_modules/**",
      "!build/**"
    ]
  }
}
